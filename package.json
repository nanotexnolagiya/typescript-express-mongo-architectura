{
  "name": "book-read",
  "version": "1.0.0",
  "description": "Application for Read Book",
  "main": "main.js",
  "repository": "http://github.com/nanotexnolagiya",
  "author": "nanotexnolagiya <nanotexnolagiya@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "clean": "rimraf dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build && npm run serve:dev",
    "serve": "node ./dist/Application.js",
    "serve:debug": "set DEBUG=app*&&node ./dist/Application.js",
    "serve:pm2": "pm2 start pm2.json",
    "serve:pm2-runtime": "pm2-runtime start pm2.json",
    "serve:dev": "set DEBUG=app*&& nodemon --inspect ./dist/Application.js",
    "stop": "pm2 stop all",
    "test": "set NODE_ENV=test&&set DEBUG=app*&&mocha --timeout 999999 --recursive dist/tests/",
    "test:ts": "set NODE_ENV=test&&set DEBUG=app*&&mocha --timeout 999999 --require ts-node/register src/tests/**/*.test.ts",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.3",
    "@types/http-errors": "^1.6.3",
    "@types/inversify": "^2.0.33",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.8",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.5",
    "@types/supertest": "^2.0.8",
    "expect": "^25.2.3",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  }
}
